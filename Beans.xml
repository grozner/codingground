<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
  
     <bean id="HelloWorld" class="com.tutorialspoint.HelloWorld">
       <property name="gilmsg" value="This message came from Beans.xml file"></property>
   </bean>
   
      <bean id="MyOtherMessage" class="com.tutorialspoint.MyOtherMessage">
       <property name="gilmsg2" value="This ***second*** also message came from Beans.xml file"></property>
   </bean>

</beans>